#define DEFINE_COMBO(NAME, BINDINGS, KEYPOS) \
  combo_##NAME { \
    timeout-ms = <50>; \
    bindings = BINDINGS; \
    key-positions = KEYPOS; \
  };

#define DEFINE_SLOW_COMBO(NAME, BINDINGS, KEYPOS) \
  combo_##NAME { \
    timeout-ms = <50>; \
    bindings = BINDINGS; \
    key-positions = KEYPOS; \
    slow-release; \
  };

