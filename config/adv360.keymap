#define KP 1
#define RFN 2
#define FN 3

// Default Kinesis Advantage 360 Pro layers.
#define KIN_DEF 4
#define KIN_KP 5
#define KIN_FN 6
#define MOD 7

#define ___ &trans
#define _x_ &none

