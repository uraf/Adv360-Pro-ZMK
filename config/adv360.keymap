    behaviors {
      #include "macros.dtsi"
      #include "version.dtsi"
      #ifndef VERSION_MACRO
      macro_ver: macro_ver {
        compatible = "zmk,behavior-macro";
        label = "macro_version";
        #binding-cells = <0>;
        bindings = <&kp RET>;
      };
      #endif

      hm: homerow_mods {
          compatible = "zmk,behavior-hold-tap";
          label = "HOMEROW_MODS";
          #binding-cells = <2>;
          tapping-term-ms = <220>;
          flavor = "tap-preferred";
          bindings = <&kp>, <&kp>;
      };

      hl: hold_layer {
          compatible = "zmk,behavior-hold-tap";
          label = "HOLD_LAYER";
          #binding-cells = <2>;
          tapping-term-ms = <220>;
          flavor = "tap-preferred";
          bindings = <&mo>, <&kp>;
